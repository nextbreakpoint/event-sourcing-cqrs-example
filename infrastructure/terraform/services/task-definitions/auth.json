[
  {
    "Name": "auth",
    "Image": "620835497377.dkr.ecr.eu-west-1.amazonaws.com/docker/auth-service:1.0.0",
    "Essential": true,
    "Memory": 600,
    "Cpu": 512,
    "PortMappings": [
      {
        "ContainerPort": 3000,
        "HostPort": 3000
      }
    ],
    "MountPoints": [
      {
        "ContainerPath" : "/config",
        "SourceVolume" : "config",
        "ReadOnly" : true
      }
    ],
    "ulimits": [
      {
        "name": "nofile",
        "hardLimit": 4096,
        "softLimit": 4096
      }
    ]
  }
]
