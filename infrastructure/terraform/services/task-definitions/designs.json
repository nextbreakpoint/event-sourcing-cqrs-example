[
  {
    "Name": "designs",
    "Image": "620835497377.dkr.ecr.eu-west-1.amazonaws.com/docker/designs-service:1.0.0",
    "Essential": true,
    "Memory": 1100,
    "Cpu": 1024,
    "PortMappings": [
      {
        "ContainerPort": 3001,
        "HostPort": 3001
      }
    ],
    "MountPoints": [
      {
        "ContainerPath" : "/config",
        "SourceVolume" : "config",
        "ReadOnly" : true
      }
    ],
    "ulimits": [
      {
        "name": "nofile",
        "hardLimit": 4096,
        "softLimit": 4096
      }
    ]
  }
]
