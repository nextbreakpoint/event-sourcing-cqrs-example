{
  "host_port": "8080",

  "server_keystore_path": "/etc/keystore_server.jks",
  "server_keystore_secret": "${KEYSTORE_SECRET}",

  "client_keystore_path": "/etc/keystore_client.jks",
  "client_keystore_secret": "${KEYSTORE_SECRET}",

  "client_truststore_path": "/etc/truststore_client.jks",
  "client_truststore_secret": "${KEYSTORE_SECRET}",

  "client_verify_host": "false",

  "jwt_keystore_path": "/etc/keystore_auth.jceks",
  "jwt_keystore_type": "jceks",
  "jwt_keystore_secret": "${KEYSTORE_SECRET}",

  "origin_pattern": "https://localhost(:[0-9]+)?",

  "client_web_url": "https://localhost",
  "client_auth_url": "https://localhost",

  "server_auth_url": "http://localhost:9000",
  "server_accounts_url": "http://localhost:9000",

  "github_url": "http://localhost:9000",
  "github_client_id": "111",
  "github_client_secret": "222",

  "oauth_login_url": "http://localhost:9000/login",
  "oauth_token_path": "http://localhost:9000/login/oauth/access_token",
  "oauth_authorize_path": "http://localhost:9000/login/oauth/authorize",
  "oauth_authority": "user:email",

  "cookie_domain": "localhost",

  "admin_users": ["admin@localhost"],

  "graphite_reporter_enabled": "false",
  "graphite_host": "localhost",
  "graphite_port": "2003"
}