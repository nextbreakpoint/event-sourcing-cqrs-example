{
  "host_port": "30101",

  "server_keystore_path": "../../secrets/keystore_server.jks",
  "server_keystore_secret": "${KEYSTORE_SECRET}",

  "client_keystore_path": "../../secrets/keystore_client.jks",
  "client_keystore_secret": "${KEYSTORE_SECRET}",

  "client_truststore_path": "../../secrets/truststore_client.jks",
  "client_truststore_secret": "${KEYSTORE_SECRET}",

  "client_verify_host": "false",

  "jwt_keystore_path": "../../secrets/keystore_auth.jceks",
  "jwt_keystore_type": "jceks",
  "jwt_keystore_secret": "${KEYSTORE_SECRET}",

  "origin_pattern": "https://localhost(:[0-9]+)?",

  "client_web_url": "https://localhost:8080",
  "client_auth_url": "https://localhost:8080",

  "server_accounts_url": "https://localhost:${ACCOUNTS_PORT}",

  "github_url": "${GITHUB_API_URL}",
  "github_client_id": "${GITHUB_CLIENT_ID}",
  "github_client_secret": "${GITHUB_CLIENT_SECRET}",

  "oauth_login_url": "${GITHUB_OAUTH_URL}/login",
  "oauth_token_path": "${GITHUB_OAUTH_URL}/login/oauth/access_token",
  "oauth_authorize_path": "${GITHUB_OAUTH_URL}/login/oauth/authorize",
  "oauth_authority": "user:email",

  "cookie_domain": "localhost",

  "admin_users": ["${ADMIN_EMAIL}"]
}