{
  "host_port": "30120",

  "server_keystore_path": "../../secrets/keystore_server.jks",
  "server_keystore_secret": "${KEYSTORE_SECRET}",

  "jwt_keystore_path": "../../secrets/keystore_auth.jceks",
  "jwt_keystore_type": "jceks",
  "jwt_keystore_secret": "${KEYSTORE_SECRET}",

  "origin_pattern": "https://localhost(:[0-9]+)?",

  "cassandra_cluster": "datacenter1",
  "cassandra_keyspace": "${DATABASE_KEYSPACE}",
  "cassandra_username": "${DATABASE_USERNAME}",
  "cassandra_password": "${DATABASE_PASSWORD}",
  "cassandra_contactPoints": "${DATABASE_HOST}",
  "cassandra_port": "9042",

  "elasticsearch_host": "${ELASTICSEARCH_HOST}",
  "elasticsearch_port": "9092",

  "s3_endpoint": "http://${MINIO_HOST}:${MINIO_PORT}",
  "s3_bucket": "${BUCKET_NAME}"
}
