{
  "host_port": "30124",

  "jwt_keystore_path": "/secrets/keystore_auth.jceks",
  "jwt_keystore_type": "jceks",
  "jwt_keystore_secret": "${KEYSTORE_SECRET}",

  "origin_pattern": "http://localhost(:[0-9]+)?",

  "kafka_bootstrap_servers": "${KAFKA_HOST}:${KAFKA_PORT}",
  "schema_registry_url": "http://${SCHEMA_REGISTRY_HOST}:${SCHEMA_REGISTRY_PORT}",
  "kafka_group_id": "designs-render",
  "message_source": "service-designs",
  "render_topic_prefix": "${RENDER_TOPIC_PREFIX}",

  "max_execution_time_in_millis": "30000000",

  "s3_endpoint": "http://${MINIO_HOST}:${MINIO_PORT}",
  "s3_bucket": "${BUCKET_NAME}"
}
